facultad <- c("Economia","Ingenieria","Arquitectura","Derecho")
licenciatura <- c("Actuaria","Ing Civil","Arquitectura","Derecho")
calumn <- c(6,4,10,8)
class(facultad)
class(licenciatura)
class(calumn)
rm(facultad)
rm(licenciatura)
etiq <- c(6,4,10,8)
pie(calumn)
pie(calumn,etiq,main="GRÁFICA DE PIE DE LA CALIFICACIÓN DE ALUMNOS",sub="Fuente: UAEMEX",col=c("red","lightblue","pink","yellow"),radius=1.3,lty=4)
